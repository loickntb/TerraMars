<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_overview">

    <fragment
        android:id="@+id/navigation_overview"
        android:name="com.example.terramars.ui.overview.OverviewFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_overview" />

    <fragment
        android:id="@+id/navigation_earth"
        android:name="com.example.terramars.ui.earth.EarthFragment"
        android:label="@string/title_Earth"
        tools:layout="@layout/fragment_earth" />

    <fragment
        android:id="@+id/navigation_mars"
        android:name="com.example.terramars.ui.mars.MarsFragment"
        android:label="@string/title_Mars"
        tools:layout="@layout/fragment_mars" />


</navigation>

